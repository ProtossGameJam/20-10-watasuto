for(var i=0; i&lt;9; i++){
    var mole = ds_map_create();
    ds_map_add(mole, "life", 0);
    ds_map_add(mole, "catched", false);
    ds_map_add(mole, "icon", 1);
    ds_map_add(mole, "bad", true);
    ds_list_add(moles, mole);
}
mole_regen_delay_var = 0.5;
