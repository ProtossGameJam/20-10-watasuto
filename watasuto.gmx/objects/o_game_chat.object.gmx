            <kind>1</kind>
            <string>/// mouse action

var click = device_mouse_check_button_pressed(0,mb_left);
var _mouse_x = device_mouse_x(0);
var _mouse_y = device_mouse_y(0);

for(var i=0; i&lt;viewer_amount; i++){
    var viewer = viewers[| i];
    if(viewer == -1)
        continue;
    
    if (click &amp;&amp; viewer[? "checked"] == false)
        if point_in_rectangle(_mouse_x, _mouse_y, left-6, top+viewer_height*i-6, left+viewer_width, top+viewer_height*i+viewer_height-6) {
            if (viewer[? "bad"]) block_bad++;
            else block_wrong++;
            
            viewer[? "checked"] = true;
        }
     
    if (viewer[? "bad"]) draw_set_color(c_red);
    else draw_set_color(c_gray);
    draw_rectangle(40+left+viewer_width*(i mod viewer_linebreak), 40+top+viewer_height*floor(i/viewer_linebreak), -40+left+viewer_width*(i mod viewer_linebreak)+viewer_width, -40+top+viewer_height*floor(i/viewer_linebreak)+viewer_height, 1);
}

var up = mouse_wheel_up() | 0;
var down = mouse_wheel_down() | 0;

if point_in_rectangle(_mouse_x, _mouse_y, 23, room_height-28-24*4-22, 23+22, room_height-28-24*4){
    chatting_regen_delay += (up-down) * 5;
}
if point_in_rectangle(_mouse_x, _mouse_y, 23, room_height-28-24*3-22, 23+22, room_height-28-24*3) {
    chatting_regen_delay_var += (up-down) * 0.1;
}
if point_in_rectangle(_mouse_x, _mouse_y, 23, room_height-28-24*1-22, 23+22, room_height-28-24*1) {
    chatting_bad_ratio += (up-down) * 5;
}
if point_in_rectangle(_mouse_x, _mouse_y, 23, room_height-28-24*0-22, 23+22, room_height-28-24*0) {
    chatting_bad_ratio_adjust_max += (up-down);
}
</string>
