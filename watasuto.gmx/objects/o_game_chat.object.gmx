        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// chatting life cycle

if (chatting_regen &gt; 0)
    chatting_regen--;
else {
    chatting_regen = chatting_regen_delay+irandom_range(-chatting_regen_delay*chatting_regen_delay_var,chatting_regen_delay*chatting_regen_delay_var);
    
    var empty_list = ds_list_create();
    for(var i=0; i&lt;viewer_amount; i++){
        var viewer = viewers[| i];
        if (viewer[? "life"] == 0)
            ds_list_add(empty_list, viewer);
    }
    if !ds_list_empty(empty_list){
        var selected = empty_list[| 0];
    }else {
        for(var i=0; i&lt;viewer_amount-1; i++){
            var viewer = viewers[| i];
            var viewer_next = viewers[| i+1];
            
            viewer[? "name"] = viewer_next[? "name"];
            viewer[? "chat"] = viewer_next[? "chat"];
            viewer[? "life"] = viewer_next[? "life"];
            viewer[? "checked"] = viewer_next[? "checked"];
            viewer[? "icon"] = viewer_next[? "icon"];
            viewer[? "bad"] = viewer_next[? "bad"];
        }
        var selected = viewers[| viewer_amount-1];
    }
    
    // 새 채팅 생성
    selected[? "life"] = chatting_life;
    selected[? "checked"] = false;
    
    if !(irandom(99) &lt; chatting_bad_ratio+chatting_bad_ratio_adjust){
        if (chatting_bad_ratio_adjust_max &gt; 0)
            chatting_bad_ratio_adjust +=  (100-chatting_bad_ratio) / (100/chatting_bad_ratio*chatting_bad_ratio_adjust_max);
        selected[? "bad"] = false;
        selected[? "icon"] = irandom_range(1,2);
    }else {
        chatting_bad_ratio_adjust = 0;
        selected[? "bad"] = true;
        selected[? "icon"] = 0;
    }
    
    selected[? "name"] = choose("무이", "잇창명", "산소", "암흑마왕 2호", "착한유저", "못된유저");
    selected[? "chat"] = choose("채팅채팅크크크크", "이건 좀 너무한거 아닌가요??", "채팅채팅채팅크크크크", "망해버려!", "넘 조아요~~~~");
    
    ds_list_destroy(empty_list);
}


for(var i=0; i&lt;viewer_amount; i++){
    var viewer = viewers[| i];
    if (viewer[? "life"] &gt; 0)
        viewer[? "life"]--;
}
