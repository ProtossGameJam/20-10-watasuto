    }
    
    draw_sprite_ext(spr_cm_icon, viewer[? "icon"], left, top + viewer_height*i-1, 0.5, 0.5, 0, c_white, draw_get_alpha());
    
    if (viewer[? "bad"]) draw_set_color(c_red);
    else draw_set_color(c_dkgray);
    
    draw_set_font(f_nanumgothic);
    draw_set_valign(fa_top)
