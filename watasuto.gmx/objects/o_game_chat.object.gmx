        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// chatting life cycle

if (instance_exists(o_room_changer)) exit;

if (chatting_regen &gt; 0)
    chatting_regen--;
else {
    chatting_regen = chatting_regen_delay+irandom_range(
        -chatting_regen_delay*chatting_regen_delay_var,
        chatting_regen_delay*chatting_regen_delay_var
    );
    
    var bad = random(1) &lt; (chatting_bad_ratio + chatting_bad_ratio_adjust)/100, chat;
    if(bad)
        chat = chat_get_bad();
    else
        chat = chat_get();
    if(!is_undefined(chat)) {
        var selected = viewers[| 0];
        if(selected == -1)
            selected = ds_map_create();
        for(var i=0; i&lt;viewer_amount-1; i++)
            viewers[| i] = viewers[| i + 1];
        viewers[| viewer_amount - 1] = selected;
        
        // 새 채팅 생성
        selected[? "checked"] = false;
        
        if(!bad) {
            if (chatting_bad_ratio_adjust_max &gt; 0)
                chatting_bad_ratio_adjust +=  (100-chatting_bad_ratio) / (100/chatting_bad_ratio*chatting_bad_ratio_adjust_max);
            selected[? "bad"] = false;
            selected[? "icon"] = irandom_range(1,2);
        } else {
            chatting_bad_ratio_adjust = 0;
            selected[? "bad"] = true;
            selected[? "icon"] = 0;
        }
        
        selected[? "name"] = choose("무이", "잇창명", "산소", "암흑마왕 2호", "착한유저", "못된유저");
        selected[? "chat"] = chat;
    }
}

if(random(1) &lt; 1/300)
    chat_set_key(choose('lol', 'clutch', 'stupid', 'poor', 'altlol', 'suddenly', 'eatrun', 'cliche', 'respects', 'spicy'));
</string>
