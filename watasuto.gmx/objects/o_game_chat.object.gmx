    var viewer = ds_map_create();
    ds_map_add(viewer, "name", "착한유저");
    ds_map_add(viewer, "chat", "채팅채팅채팅채팅크크크크");
    ds_map_add(viewer, "life", 0);
    ds_map_add(viewer, "checked", false);
    ds_map_add(viewer, "bad", false);
    ds_list_add(viewers, viewer);
}
