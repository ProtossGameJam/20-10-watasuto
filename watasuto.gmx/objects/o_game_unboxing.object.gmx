            <string>spr_w = sprite_get_width(spr_unboxing_box);
spr_h = sprite_get_height(spr_unboxing_box);

left = room_width / 2 - spr_w / 2 + 40;
top = room_height / 2 - spr_h / 2 + 40;

timer = 0;
timer_max = room_speed * 60; // 임시 게임오버 조건

box_opening = 0;
